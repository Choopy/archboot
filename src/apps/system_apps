#!/bin/bash

# Intel Microcode (only for Intel CPU)
export _intel="intel-ucode"

# Network
export _networkapps="networkmanager dhclient iw wpa_supplicant wireless_tools"

# Required
export _requiredapps="ntp wget reflector git htop nmap numlockx vim"

# Bootloader
export _bootapps="grub os-prober gptfdisk efibootmgr"

# yaourt
export _yaourtapps="yaourt customizepkg"
